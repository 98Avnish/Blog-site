<%- include("partials/header"); -%>

  <h1>Compose</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label for="blogTitle">Title</label>
      <input type="text" name="blogTitle" class="form-control" required>
      <label for="blogBody">Post</label>
      <textarea name="blogBody" class="form-control" cols="40" rows="5"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
  </form>

<%- include("partials/footer"); -%>
